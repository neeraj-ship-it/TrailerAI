{
  "name": "stage-nest-backend",
  "version": "2.0.0",
  "description": "Stage nest backend server",
  "author": "Stage Tech Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=5000' nest build",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --ignore-path .prettierignore --check \"**/*.{ts,js,json,md}\"",
    "start": "nest start",
    "build:sdk": "rimraf ../../src/api/functional && cd ../.. && npx nestia sdk && cd packages/api",
    "start:dev": "NODE_OPTIONS='--max-old-space-size=4096' nest start --watch",
    "start:debug": "NODE_OPTIONS='--max-old-space-size=4096' nest start --debug --watch",
    "start:prod": "NODE_OPTIONS='--require dd-trace/init' node dist/src/main",
    "lint:check": "eslint \"**/*.ts\" --fix --ignore-pattern 'dist/**'",
    "type:check": "tsc --pretty --noEmit",
    "lint:fix": "eslint \"**/*.ts\" --fix --ignore-pattern 'dist/**'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --config jest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --clearCache && NODE_OPTIONS='--max-old-space-size=8000' jest --config jest.config.ts --detectOpenHandles --forceExit --logHeapUsage --runInBand",
    "test:e2e:summary": "jest --config jest.config.ts --detectOpenHandles --forceExit --logHeapUsage 2>&1 | grep -e 'FAIL' -e '●' -e '✕' -e '✓' -e 'MODULE' -e 'e2e-spec.ts'",
    "test:e2e:fail": "jest --config jest.config.ts --detectOpenHandles --forceExit --logHeapUsage 2>&1 | grep -e 'FAIL' -e '●' -e '✕'",
    "seed:characters": "ts-node -r tsconfig-paths/register src/chat/scripts/seed-characters.ts",
    "verify:characters": "ts-node -r tsconfig-paths/register src/chat/scripts/verify-characters.ts",
    "prepare": "ts-patch install && typia patch && (command -v husky >/dev/null 2>&1 && husky || true)"
  },
