# Stage Admin - Frontend Environment Variables
# Copy this file to .env and fill in the values

# Backend API URL (NestJS server)
NEXT_PUBLIC_BASE_API_URL=http://localhost:3020

# Secondary/legacy APIs (optional for local dev)
NEXT_PUBLIC_SECONDARY_API_URL=
NEXT_PUBLIC_EXPRESS_BASE_API_URL=

# CDN for media assets (optional for local dev)
NEXT_PUBLIC_CDN_ROOT_URL=

# CMS API (optional for local dev)
NEXT_PUBLIC_CMS_BASE_API_URL=

# AWS ECS (not needed for local dev)
AWS_ECS_TASK_DEFINITION_AVATAR_RENDER=
